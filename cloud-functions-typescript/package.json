{
  "name": "cloud-functions-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc-watch --onSuccess 'functions-framework --target=TypescriptFunction'",
    "start": "functions-framework --target=TypescriptFunction",
